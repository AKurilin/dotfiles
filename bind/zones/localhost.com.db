; BIND data file for localhost.com
;
$TTL 604800
@           IN     SOA     ns.localhost.com. root.localhost.com. (
                                6                   ; Serial
                           604800                   ; Refresh
                            86400                   ; Retry
                          2419200                   ; Expire
                           604800 )                 ; Negative Cache TTL
;
@           IN     NS      ns.localhost.com.
ns          IN     A       192.168.131.181
alex        IN     A       192.168.131.181

www         IN     CNAME   alex
api         IN     CNAME   alex
classroom   IN     CNAME   alex
db          IN     CNAME   alex
school      IN     CNAME   alex
student     IN     CNAME   alex
